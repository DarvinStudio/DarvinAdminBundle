    "require": {
        "php":                              ">=5.4",
        "symfony/symfony":                  ">=2.7",
        "doctrine/orm":                     "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal":                    "<2.5",
        "doctrine/doctrine-bundle":         "~1.4",
        "knplabs/knp-paginator-bundle":     "2.4.*",
        "ocramius/proxy-manager":           "1.0.*",
        "stof/doctrine-extensions-bundle":  "1.1.*",
        "willdurand/js-translation-bundle": "2.2.*",
